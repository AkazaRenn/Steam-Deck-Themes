{
    "name": "Skinny Window Header",
    "version": "v1.2",
    "target": "Desktop-Library",
    "author": "SuchMeme",
    "manifest_version": 7,
    "description": "Compacts the top bar of Steam Desktop",
    "tabs": {
        "default": ["Steam"]
    },
    "inject": {
        "steam.css": []
    },
    "patches": {
        "Use Custom Icons": {
            "type": "checkbox",
            "values": {
                "Yes": {
                    "custom_icons.css": []
                },
                "No": {}
            }
        }
    }
}