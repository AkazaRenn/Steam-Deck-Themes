{
  "name": "No Hero Move",
  "author": "Akaza Renn",
  "manifest_version": 5,
  "version": "v1.0",
  "inject": {
    "shared.css": ["SP"]
  },
  "patches": {
    "Lift Hero": {
      "default": "0",
      "type": "slider",
      "values": {
        "0": {
          "--NHM-lift-hero-px": ["0", "SP"]
        },
        "1": {
          "--NHM-lift-hero-px": ["10px", "SP"]
        },
        "2": {
          "--NHM-lift-hero-px": ["20px", "SP"]
        },
        "3": {
          "--NHM-lift-hero-px": ["30px", "SP"]
        },
        "4": {
          "--NHM-lift-hero-px": ["40px", "SP"]
        },
        "5": {
          "--NHM-lift-hero-px": ["50px", "SP"]
        },
        "6": {
          "--NHM-lift-hero-px": ["60px", "SP"]
        },
        "7": {
          "--NHM-lift-hero-px": ["70px", "SP"]
        },
        "8": {
          "--NHM-lift-hero-px": ["80px", "SP"]
        },
        "9": {
          "--NHM-lift-hero-px": ["90px", "SP"]
        },
        "10": {
          "--NHM-lift-hero-px": ["100px", "SP"]
        }
      }
    }
  }
}